        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/ExpeditionEditDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/PassengersListDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/AddCompanyDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/DeleteCompanyDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/UpdateCompanyDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/DeleteUserDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/UpdateUserDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/CampaignRemoveDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/CampaignAddDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/CampaignUpdateDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/AddAdminDialogAnimation</item>
        <item name="android:windowAnimationStyle" tools:ignore="DuplicateDefinition">@style/DeleteAdminDialogAnimation</item>

    </style>

